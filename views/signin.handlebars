<!--Start of Form-->
<div class="row">
    <div class="col-md-7 mx-auto">
        <div class="card" id="signUp">
            <h2>Login</h2>
            <form id="register-form" method="POST" action="/signin">
                

                <div class="form-group">
                    <label for="email">Email</label>
                    <input type="email" class="form-control" id="email" placeholder="name@email.com" name="email">
                </div>
                <div class="form-group">
                    <label for="password">Password</label>
                    <input type="password" class="form-control" id="password" name="password">
                </div>

                
                    <button type="submit" id="submit-btn">Play!</button>
                    <small>Signup <a href="/signup">here</a> if you don't have an account!</small>
            </form>
            </div>
        </div>
    </div>

    <script src="/scripts/signupScript.js"></script>